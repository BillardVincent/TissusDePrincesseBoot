
INSERT INTO PUBLIC.TISSU_REQUIS_GAMME_POIDS 
(TISSU_REQUIS_ID , GAMME_POIDS)
SELECT DISTINCT
tr.ID, tr.GAMME_POIDS
FROM TISSU_REQUIS tr;

INSERT INTO PUBLIC.PATRON_VERSION 
(NOM, PATRON_ID)
SELECT DISTINCT
'VERSION', p.ID
FROM PATRON p;




