                  <Insets top="5.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER" prefHeight="83.0" prefWidth="126.0">
               <children>
                  <ImageView fx:id="image" fitHeight="95.0" fitWidth="118.0" pickOnBounds="true" preserveRatio="true" styleClass="light-background" />
               </children>
            </HBox>
         </children>
         <styleClass>
            <String fx:value="global" />
            <String fx:value="card" />
         </styleClass>
      </VBox>
   </children>
