<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="183.0" prefWidth="410.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.vbillard.tissusdeprincesseboot.controller.rangement.NewRangementModale">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="410.0" spacing="20.0">
         <children>
            <Label text="Nom du nouveau rangement :" />
            <VBox alignment="CENTER" prefHeight="54.0" prefWidth="310.0" spacing="5.0">
               <children>
                  <JFXTextField fx:id="champ" prefHeight="25.0" prefWidth="297.0" promptText="nouveau" />
                  <Label fx:id="warning" text="warning" />
               </children>
            </VBox>
            <HBox alignment="CENTER" prefHeight="28.0" prefWidth="410.0" spacing="20.0">
               <children>
                  <JFXButton onAction="#handleOk" text="OK" />
                  <JFXButton onAction="#handleCancel" text="Annuler" />
               </children>
            </HBox>
         </children>
         <opaqueInsets>
            <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
         </opaqueInsets>
         <padding>
            <Insets bottom="20.0" left="50.0" right="50.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
